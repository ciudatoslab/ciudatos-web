---
import Layout from "../../layouts/Layout.astro";
interface Props {
  title: string;
  img: string;
  description: string;
}
const { title, img, description } = Astro.props;
---

<Layout title={title} img={img} description={description}>
  <div class="flex flex-col gap-5 sm:gap-9 px-4 sm:px-0">
    <slot />
  </div>
</Layout>
